    <hr/>
    <div>
        <p>
            <span>Planet:</span>
            <span>{{name}}</span>
        </p>
    </div>
        <div>
            <p>
                <span>Rotation Period: </span>
                <span>{{rotation_period}} years</span>
            </p>
        </div>
{{/each}}